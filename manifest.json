{
    "manifest_version": 2,
        "name": "Profanity Detection",
        "description": "This extension censores vulgar and offensive words.",
        "version": "1.0",
        "content_scripts": [
            {
                "matches": [
                    "<all_urls>"
                ],
                "js": [
                    "content.js"
                ],
                "run_at": "document_end"
            }
        ]
    }
    